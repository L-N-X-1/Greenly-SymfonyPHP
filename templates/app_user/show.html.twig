{% extends 'base.html.twig' %}

{% block title %}AppUser{% endblock %}

{% block body %}
    <h1>AppUser</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ app_user.id }}</td>
            </tr>
            <tr>
                <th>User_id</th>
                <td>{{ app_user.userId }}</td>
            </tr>
            <tr>
                <th>Role</th>
                <td>{{ app_user.role }}</td>
            </tr>
            <tr>
                <th>User_name</th>
                <td>{{ app_user.userName }}</td>
            </tr>
            <tr>
                <th>User_password</th>
                <td>{{ app_user.userPassword }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ app_user.email }}</td>
            </tr>
            <tr>
                <th>Creation_date</th>
                <td>{{ app_user.creationDate }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_user_index') }}">back to list</a>

    <a href="{{ path('app_user_edit', {'id': app_user.id}) }}">edit</a>

    {{ include('app_user/_delete_form.html.twig') }}
{% endblock %}
